<%= form_for(@project) do |f| %>
	
	<div class='field'>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</div>

	<div class="field">
		<%= f.label :description %><br>
		<%= f.text_area :description %>
	</div>

	<div class="field">
		<%= f.label :url %><br>
		<%= f.text_area :url %>
	</div>

	<div class"action">
		<%= f.submit %>
	</div>
<% end %>